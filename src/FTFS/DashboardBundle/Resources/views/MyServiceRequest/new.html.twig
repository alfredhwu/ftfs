{% extends "FTFSDashboardBundle::layout.html.twig" %}

{% block body_content %}
{% include 'FTFSDashboardBundle:MyServiceRequest:new_content.html.twig'%}
{% endblock %}

{% block javascript %}
    {{ parent() }}
    <script>
        function submitform(initmode)
        {
            form = document.forms["{{ prefix~'_form' }}"];
            form.init_mode.value = initmode;
            form.submit();
        }
    </script>
{% endblock %}
